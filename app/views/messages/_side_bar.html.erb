<div class = "new_chatroom">
  <div class = "username">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class = "room_btn">
    <%= link_to "チャットルームを作成する", new_room_path %>
  </div>
</div>

<div class = "chat_overview">
  <div class = 'rooms'>
    <%# ログイン中のユーザーが参加しているRoomを出すから、current_user.rooms.eachになる%>
    <% current_user.rooms.each do |room|%>
      <div class = 'room'>
        <div class = "room_name">
          <a href = "#"><%= room.name%></a>
        </div>
      </div>
    <% end  %>
  </div>
</div>